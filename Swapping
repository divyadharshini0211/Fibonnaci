package task;

public class swapping {

	public static void main(String[] args) {
		
		        int a = 15;
		        int b = 10;

		        System.out.println("Before swap:");
		        System.out.println("a = " + a);
		        System.out.println("b = " + b);

		 
		        int temp = a;
		        a = b;
		        b = temp;

		        System.out.println("\nAfter swap:");
		        System.out.println("a = " + a);
		        System.out.println("b = " + b);
		    }
		


	}

